{
    "name": "synchronik",
    "version": "2.3.2",
    "private": false,
    "description": "A modular orchestration engine for building resilient, milestone-driven workflows in Node.js.",
    "homepage": "https://github.com/owen-6936/synchronik#readme",
    "bugs": {
        "url": "https://github.com/owen-6936/synchronik/issues"
    },
    "keywords": [
        "orchestration",
        "workflow",
        "engine",
        "task management",
        "worker pool",
        "concurrency",
        "parallelism",
        "automation",
        "nodejs",
        "typescript",
        "event-driven",
        "resilience"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/owen-6936/synchronik.git"
    },
    "license": "MIT",
    "author": "Owen â€¢ Nexicore Digital",
    "contributors": [
        {
            "name": "Owen",
            "email": "erhaborowen3@gmail.com",
            "url": "https://github.com/owen-6936"
        }
    ],
    "publisher": "owen-6936",
    "engines": {
        "node": ">=18.0.0"
    },
    "type": "module",
    "main": "dist/synchronizer.js",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "scripts": {
        "test": "vitest run --cache",
        "test:watch": "vitest --watch --cache",
        "prebuild": "rm -rf dist && tsc --noEmit && eslint . --ext .ts --cache && vitest run",
        "start": "node dist/index.js",
        "dev": "ts-node src/index.ts",
        "build": "tsup src/index.ts --format esm,cjs --dts --clean",
        "lint": "eslint . --ext .ts --cache --fix",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write --cache .",
        "docs": "typedoc",
        "example:basic": "tsx examples/1-basic-worker.ts",
        "example:advancedv1": "tsx examples/2-advanced-process.ts",
        "example:advancedv2": "tsx examples/3-showcase-physics.ts",
        "example:advancedv3": "tsx examples/4-resilient-scraper.ts",
        "example:advancedv4": "tsx examples/5-multi-stage-pipeline.ts",
        "example:advancedv5": "tsx examples/6-health-monitor.ts",
        "example:advancedv6": "tsx examples/7-dynamic-health-monitor.ts",
        "example:advancedv7": "tsx examples/8-status-tracker-usage.ts",
        "example:advancedv8": "tsx examples/9-dependency-graph-usage.ts",
        "example:persistence": "tsx examples/10-persistent-engine.ts"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@swc/cli": "^0.7.9",
        "@swc/core": "^1.15.3",
        "@types/express": "^5.0.5",
        "@types/jest": "^30.0.0",
        "@types/node": "^24.10.1",
        "@types/papaparse": "^5.5.0",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "@vitejs/plugin-react": "^5.1.1",
        "eslint": "^9.39.1",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-vitest": "^0.5.4",
        "jsdom": "^27.2.0",
        "prettier": "^3.7.2",
        "tsup": "^8.5.1",
        "tsx": "^4.20.6",
        "typedoc": "^0.28.15",
        "typescript": "^5.9.3",
        "vitest": "^4.0.14"
    }
}
